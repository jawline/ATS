fn even(n) -> (if n (odd (- n 1)) true)
fn odd(n) -> (if n (even (- n 1)) false)
fn fact(n) -> (if n (* n (fact (- n 1))) 1)

fn pow(n,r) -> (if r (* n (pow n (- r 1))) 1)

fn pzc(n,r) -> (r)

fn identity(x) -> (x)

